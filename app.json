{
  "expo": {
    "name": "hcp-patient",
    "slug": "hcp-patient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/drugSpec.webp",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/drugSpec.webp",
        "backgroundColor": "#ffffff"
      },
      "package": "com.taslim121.hcppatient"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./src/assets/images/drugSpecIconp.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/drugSpec.webp",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "70b4a9e1-d1dc-4350-9d1f-a32c09321799"
      }
    },
    "owner": "taslim121"
  }
}
